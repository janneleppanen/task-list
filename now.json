{
  "name": "system-information",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@now/next"
    },
    {
      "src": "backend/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
