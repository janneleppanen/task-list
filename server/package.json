{
  "name": "server",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "npm run app:migrate && npm run test:migrate",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --runInBand",
    "test:start": "nodemon --watch '**/*.js' --exec 'node' index.js",
    "test:migrate": "NODE_ENV=test knex migrate:latest --env test --knexfile ./database/migrate.js",
    "test:travis": "npm run test",
    "app:migrate": "knex migrate:latest --knexfile ./database/migrate.js",
    "app:rollback": "knex migrate:rollback --knexfile ./database/migrate.js",
    "start": "nodemon -e js,graphql src/index.js",
    "heroku-prebuild": "npm install && npm run app:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.16.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^5.10.1",
    "graphql": "^14.7.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.20.15",
    "nodemon": "^2.0.4",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.54",
    "babel-jest": "^26.3.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "jest": "^26.4.1",
    "node-fetch": "^2.6.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "14.x"
  }
